.all{
  position: relative;
}
page{
  background-color: #f1f1f1;
}
.team {
  color: #f1f1f1;
}
.bg{
  width: 100%;

}
.teamIcon {
 
  width: 100%;
  height: 563rpx;
  background-color: rgba(0,0,0,0.3);
  position: absolute;
  padding-top: 80rpx;
  box-sizing: border-box;
  top: 0;
}

.pic {
  width: 140rpx;
  height: 140rpx;
  margin: 0rpx auto
}

.pic image {
  width: 140rpx;
  height: 140rpx;
  border-radius: 50%;
  /* background-color: #000; */
  text-align: center;
  margin-top: 30rpx;
}

.name {
  font-weight: 700;
  text-align: center;
  padding: 50rpx 0 26rpx 0;
  font-size: 36rpx;
}

.col {

  padding: 0 20px;
  border-radius: 8rpx;
  box-sizing: border-box;
  padding: 30rpx 0;
  width: 550rpx;
  margin: 0 auto;
  text-align: center;

}

.col view {
  display: inline-block;
}

.line {
  /* width:4rpx; */
  height: 28rpx;
  /* background-color: #ccc; */
  border: 1rpx dashed #ccc;
  margin: 0 50rpx;
  vertical-align: middle;
}

.slogn {
  text-align: center;
}
.con{
  /* display: flex; */
}
.userList{
/* flex:1; */
/* overflow: auto; */
background-color:#f4f4f4;
padding: 0 20rpx;
box-sizing: border-box;

}
.userList .box{
width:100%;
display: flex;
height: 160rpx;
margin-bottom: 10rpx;
